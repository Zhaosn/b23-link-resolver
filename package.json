{
  "name": "b23-link-resolver",
  "version": "1.0.0",
  "description": "Obsidian plugin to resolve b23.tv short links",
  "main": "main.js",
  "scripts": {
    "build": "tsc --noEmit --skipLibCheck && esbuild main.ts --bundle --outfile=main.js --external:obsidian --format=cjs --platform=node",
    "dev": "esbuild main.ts --bundle --outfile=main.js --external:obsidian --format=cjs --platform=node --watch"
  },
  "keywords": [
    "obsidian",
    "bilibili",
    "link"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.19.0",
    "esbuild": "^0.20.0",
    "obsidian": "^1.4.0",
    "typescript": "^5.2.2"
  }
}
