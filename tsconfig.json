{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES6",
    "lib": ["ES6", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "outDir": ".",
    "noEmit": true,
    "types": ["node", "obsidian"]
  },
  "include": ["main.ts"],
  "exclude": ["node_modules", "**/*.js"]
}